{
  "name": "boilerPlate-node-coffee-ava",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "ava",
    "compile": "coffee -o ./ -cw coffee/",
    "watch:ava": "chokidar 'lib/*.js' 'test/*.js' -c 'nyc ava'",
    "watch": "npm run compile &node watch.js &npm run watch:ava",
    "cover": "nyc ava",
    "report": "nyc report --reporter=html",
    "update:snap": "ava --update-snapshots"
  },
  "keywords": [],
  "dependencies": {
    "commander": "^2.15.1",
    "rrq": "^1.1.1",
    "xza": "^2.0.1"
  },
  "devDependencies": {
    "chokidar": "^2.0.3",
    "chokidar-cli": "^1.2.0",
    "coffeescript": "^2.2.3"
  },
  "coffeelintConfig": {
    "max_line_length": {
      "level": "ignore"
    },
    "colon_assignment_spacing": {
      "level": "warn",
      "spacing": {
        "left": 0,
        "right": 1
      }
    },
    "missing_fat_arrows": {
      "level": "warn"
    },
    "no_interpolation_in_single_quotes": {
      "level": "warn"
    },
    "no_unnecessary_fat_arrows": {
      "level": "ignore"
    },
    "no_unnecessary_double_quotes": {
      "level": "warn"
    }
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "**/*.js"
    ],
    "failFast": false,
    "serial": false,
    "tap": false,
    "verbose": false,
    "color": true,
    "watch": false
  },
  "main": "lib/index.js",
  "license": "MIT",
  "author": "mick-whats"
}
